{
  "compilerOptions": {
      "outDir": "./built/",
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "module": "es2015",
      "moduleResolution": "node",
      "target": "es6",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "./*"
        ]
      }
  },
  "include": [
      "./src/**/*"
  ]
}